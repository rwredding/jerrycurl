<?xml version="1.0" encoding="utf-8"?>
<package xmlns="http://schemas.microsoft.com/packaging/2013/05/nuspec.xsd">
    <metadata>
        <id>$PackageId$</id>
        <title>$PackageId$</title>
        <version>$PackageVersion$</version>
        <authors>$PackageAuthors$</authors>
        <owners>$PackageOwners$</owners>
        <copyright>$PackageCopyright$</copyright>
        <license type="expression">$PackageLicense$</license>
        <icon>$PackageIconFile$</icon>
        <iconUrl>$PackageIconUrl$</iconUrl>
        <projectUrl>$PackageProjectUrl$</projectUrl>
        <tags>$PackageTags$</tags>
        <repository type="git" url="$RepositoryUrl$" commit="$GitCommitId$" />
        <requireLicenseAcceptance>false</requireLicenseAcceptance>
        <description>JSON support for Jerrycurl. Targets System.Text.Json.</description>
        <dependencies>
            <group targetFramework="netstandard2.0">
                <dependency id="System.Text.Json" version="$JsonPackageVersion$" />
                <dependency id="Jerrycurl.Mvc" version="$PackageVersion$" />
            </group>
            <group targetFramework="netstandard2.1">
                <dependency id="System.Text.Json" version="$JsonPackageVersion$" />
                <dependency id="Jerrycurl.Mvc" version="$PackageVersion$" />
            </group>
            <group targetFramework="netcoreapp3.0">
                <dependency id="Jerrycurl.Mvc" version="$PackageVersion$" />
            </group>
        </dependencies>
    </metadata>
    <files>
        <file src="$IconPath$" target="" />
        <file src="$BinPath$**\Jerrycurl.Extensions.Json.*" target="lib" />
    </files>
</package>